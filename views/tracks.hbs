<h1>Tracks</h1>

<div class="container">

  {{#each trackAlbum}}

    {{!-- <h2>{{this.name}}</h2> --}}

    <figure>
      <figcaption>Listen to {{this.name}}:</figcaption>
      <audio controls src="{{this.preview_url}}">
        <a href="{{this.preview_url}}">
          Download audio
        </a>
      </audio>
    </figure>

  {{/each}}

</div>